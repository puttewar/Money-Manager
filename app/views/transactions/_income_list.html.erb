<table class="table table-striped table-hover ">
  <thead>
	  <tr class="info">
		  <th> Date </th>
		  <th> From </th>
		  <th> To</th>
      <th> Amount</th>
		  <th> Description</th>
		  <th> Transaction Type</th>
	  </tr>
  </thead>
  <tbody>
    <% @transaction.each do |e| %>
      <tr>
        <% if e.transable_type.present? && e.transable_type == "Income"%>
          <td> <%= e.date %> </td>
          <td> <%= e.from %> </td>
          <td> <%= e.to %> </td>
          <td> <%= e.amount %> </td>
          <td> <%= e.description %> </td>
          <td> <%= e.transable_type %> </td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>