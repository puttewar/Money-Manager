<table class="table table-striped table-hover ">
  <thead>
	  <tr class="info">
		  <th> Category Name </th>
		  <th> Description </th>
          <th> Show </th>
		  <th> Edit </th>
		  <th> Delete </th>
		  <th></th>
	  </tr>
  </thead>
  <tbody>
    <% @expence_category.each do |e| %>
      <tr>
        <td> <%= e.expence_source %> </td>
        <td> <%= e.description %> </td>
        <td> <%= link_to "show",expence_category_show_path(e) %></td>
        <td> <%= link_to "Edit",expence_category_edit_path(e) %></td>
        <td> <%= link_to "Delete",expence_category_destroy_path(e), method: :delete, data: { confirm:'are u sure?' } %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>